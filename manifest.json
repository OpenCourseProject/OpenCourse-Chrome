{
  "manifest_version": 2,

  "name": "Open Course",
  "description": "Open Course Project browser plugin",
  "version": "1.3",

  "permissions": [
    "https://opencourseproject.com/"
  ],
  "content_scripts": [ {
    "js": [ "assets/js/jquery-1.11.1.min.js", "assets/js/background.js"],
    "matches": [ "https://burke.cnu.edu:9997/*" ]
    }],
  "background": {
      "scripts": ["assets/js/query.js"],
      "persistent": false
  },
  "icons": { "16": "assets/img/icon16.png",
    "48": "assets/img/icon48.png",
    "128": "assets/img/icon128.png" },
  "web_accessible_resources": [ "assets/*" ]
}
