{
  "manifest_version": 2,

  "name": "OpenCourse",
  "description": "Open Course Project browser plugin",
  "version": "1.1",

  "permissions": [
    "https://opencourseproject.com/",
    "storage"
  ],
  "content_scripts": [ {
    "js": [ "jquery-1.11.1.min.js", "background.js", "assets/js/bootstrap.js" ],
    "matches": [ "https://burke.cnu.edu:9997/*" ]
    }],
  "background": {
      "scripts": ["query.js"],
      "persistent": false
  },
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
  "web_accessible_resources": [ "assets/*" ]
}
